{# empty Twig template #}

{% block user_widget %}

    <div class="panel panel-default">
        <div class="panel-heading">{{ form_label(form) }}</div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.username) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.sername) }}
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.phone) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.email) }}
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    {{ form_row(form.skype) }}
                </div>
                <div class="col-md-6">
                    {{ form_row(form.viber) }}
                </div>
            </div>
        </div>
    </div>






{% endblock %}

